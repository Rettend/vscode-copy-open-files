{
  "publisher": "Rettend",
  "name": "vscode-copy-open-files",
  "displayName": "vscode-copy-open-files",
  "type": "module",
  "version": "0.0.4",
  "private": true,
  "packageManager": "bun@1.3.0",
  "description": "Copy the contents of all open files",
  "repository": {
    "type": "git",
    "url": "https://github.com/rettend/vscode-copy-open-files"
  },
  "categories": [
    "Other"
  ],
  "main": "./out/extension.cjs",
  "engines": {
    "vscode": "^1.99.0"
  },
  "activationEvents": [
    "onStartupFinished"
  ],
  "contributes": {},
  "scripts": {
    "vscode:prepublish": "bun run build",
    "build": "tsdown",
    "check": "tsgo -p tsconfig.json --noEmit",
    "lint": "eslint . --fix",
    "update": "taze latest -wr",
    "package": "vsce package --no-dependencies",
    "release": "bumpp && bun run package"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^6.0.0",
    "@types/bun": "^1.3.0",
    "@types/vscode": "^1.99.0",
    "@typescript/native-preview": "^7.0.0-dev.20251021.1",
    "@vscode/vsce": "^3.6.2",
    "bumpp": "^10.3.1",
    "eslint": "^9.38.0",
    "ignore": "^7.0.5",
    "taze": "^19.8.1",
    "tsdown": "latest",
    "typescript": "^5.9.3"
  }
}
